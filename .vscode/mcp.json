{
  "servers": {
    "google-maps": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "GOOGLE_MAPS_API_KEY",
        "mcp/google-maps"
      ],
      "env": {
        "GOOGLE_MAPS_API_KEY": "google_maps_api_key"
      }
    },
    "bigquery": {
        "command": "./mcp_toolbox_database/toolbox.exe",
        "args": ["--prebuilt","bigquery","--stdio"],
        "env": {
        "BIGQUERY_PROJECT": "main-analog-487920-c9"
      }
    }
  }
}